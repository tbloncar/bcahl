<div class="row">
	<div class="page-header span12">
		<h1>Edit Team: <%= @team.name %></h1>
	</div>
</div>
<div class="row">
	<div class="span6">
		<%= render 'form', form_action: team_url(@team.path), form_method: "patch", form_button: "Update Team"  %>
	</div>
	<div class="span6">
		<%= link_to "Delete Team", team_url(@team.path), method: :delete %>
	</div>
</div>